{
  "version": "1.1.0",
  "name": "promise-wtf",
  "description": "Lightweight Promise implementation with the 'finally' method",
  "homepage": "https://www.npmjs.com/package/promise-wtf",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/promise-wtf"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint ./src ./test",
    "pretest": "npm run lint && snyk test && npm run aplus",
    "test": "nyc tape test/start.js | tap-spec",
    "posttest": "nyc report --reporter=lcov",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls",
    "build": "gccmin src dist",
    "aplus": "node ./test/aplus/index.js"
  },
  "devDependencies": {
    "coveralls": "latest",
    "eslint": "latest",
    "gcc-min": "latest",
    "eslint-config-ggc": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "promises-tests": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "library",
    "promise",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/promise-wtf/issues"
  },
  "directories": {
    "test": "test"
  }
}
