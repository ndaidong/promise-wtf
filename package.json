{
  "version": "1.2.2",
  "name": "promise-wtf",
  "description": "Lightweight Promise implementation with the 'finally' method",
  "homepage": "https://www.npmjs.com/package/promise-wtf",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/promise-wtf"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint .",
    "snyk": "snyk test",
    "tape": "nyc tape test/start.js | tap-spec",
    "report": "nyc report --reporter=lcov",
    "pretest": "npm run lint && npm run snyk",
    "test": "npm run aplus && npm run tape",
    "posttest": "npm run report",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls",
    "aplus": "node ./test/aplus/index.js",
    "build": "gccmin"
  },
  "devDependencies": {
    "coveralls": "latest",
    "eslint": "latest",
    "gcc-min": "latest",
    "eslint-config-ggc": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "promises-tests": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "gccmin": {
    "source": "src",
    "target": "dist",
    "filename": "promise-wtf",
    "globalVar": "Promise"
  },
  "keywords": [
    "library",
    "promise",
    "util"
  ],
  "license": "MIT"
}
