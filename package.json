    "snyk": "snyk wizard"
  },
  "devDependencies": {
    "coveralls": "latest",
    "eslint": "latest",
    "gcc-min": "latest",
    "nyc": "latest",
    "promises-tests": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "library",
